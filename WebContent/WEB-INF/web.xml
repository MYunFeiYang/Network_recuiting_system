<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         id="WebApp_ID" version="3.1">
    <display-name>test</display-name>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>person</servlet-name>
        <servlet-class>controller.servletPerson</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>person</servlet-name>
        <url-pattern>/person</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>enterprise</servlet-name>
        <servlet-class>controller.servletEnterprise</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>enterprise</servlet-name>
        <url-pattern>/enterprise</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>public</servlet-name>
        <servlet-class>controller.servletPublic</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>public</servlet-name>
        <url-pattern>/public</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>query</servlet-name>
        <servlet-class>controller.servletQuery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>query</servlet-name>
        <url-pattern>/query/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>admin</servlet-name>
        <servlet-class>controller.servletAdmin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>admin</servlet-name>
        <url-pattern>/admin/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>fileLoad</servlet-name>
        <servlet-class>controller.servletFileLoad</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>fileLoad</servlet-name>
        <url-pattern>/fileLoad/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ValidateCodeServlet</servlet-name>
        <servlet-class>controller.ValidateCodeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ValidateCodeServlet</servlet-name>
        <url-pattern>/ValidateCodeServlet</url-pattern>
    </servlet-mapping>
</web-app>